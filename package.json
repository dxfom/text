{
  "name": "@dxfom/text",
  "version": "0.2.0",
  "description": "A low level DXF TEXT content parser.",
  "license": "GPL-3.0",
  "repository": "dxfom/text",
  "keywords": [
    "dxf"
  ],
  "main": "index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -p . && terser -c -m --toplevel -b beautify=false,semicolons=false --warn -o index.mjs index.js"
  },
  "devDependencies": {
    "terser": "^5.5.1",
    "typescript": "^4.1.3"
  }
}
